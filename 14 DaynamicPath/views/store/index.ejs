<%- include('../partials/head') %>
  </head>
  <body>
    <%- include('../partials/nav') %>
    <main class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl">
      <h2 class="text-3xl text-red-500 font-bold text-center mb-6">
        Welcome to airbnb index page:
      </h2>
      <ul class="space-y-8">
        <% registeredHomes.forEach(home => { %>
         <div class="bg-white rounded-lg shadow-md overflow-hidden hover:shadow-lg transition duration-300 w-full max-w-sm">Add commentMore actions
            <img src="https://www.bing.com/images/search?view=detailV2&ccid=ZHOPK0je&id=A98595DC8BBD84AD0CF6CF124457C28C0312E8BB&thid=OIP.ZHOPK0jeut0Dmkn7G8XQCwHaE8&mediaurl=https%3A%2F%2Fa0.muscache.com%2Fim%2Fpictures%2Fmiso%2FHosting-52329661%2Foriginal%2F800aa635-16b7-4ceb-adbc-3d2445edd9dd.jpeg%3Fim_w%3D720&exph=480&expw=720&q=airbnb+india&simid=607997474216174008&form=IRPRST&ck=B7A71444700073CA506CCD00B66C9903&selectedindex=6&itb=0&cw=1222&ch=585&ajaxhist=0&ajaxserp=0&vt=0&sim=11" alt="<%= home.houseName %>" class="w-full h-48 object-cover">
            <div class="p-4">
              <h3 class="text-xl font-semibold text-gray-800 mb-2"><%= home.houseName %></h3>
              <p class="text-gray-600 mb-2"><i class="fas fa-map-marker-alt mr-2"></i><%= home.location %></p>
              <div class="flex justify-between items-center mb-2">
                <span class="text-lg font-bold text-red-500">Rs<%= home.price %> / night</span>
                <div class="flex items-center">
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5 text-yellow-400 mr-1">
                    <path fill-rule="evenodd" d="M10.788 3.21c.448-1.077 1.976-1.077 2.424 0l2.082 5.007 5.404.433c1.164.093 1.636 1.545.749 2.305l-4.117 3.527 1.257 5.273c.271 1.136-.964 2.033-1.96 1.425L12 18.354 7.373 21.18c-.996.608-2.231-.29-1.96-1.425l1.257-5.273-4.117-3.527c-.887-.76-.415-2.212.749-2.305l5.404-.433 2.082-5.006z" clip-rule="evenodd" />
                  </svg>
                  <span class="text-gray-700"><%= home.rating %></span>
                </div>
              </div>
              <button class=""bg-green-500 p-2>Book</button>
            </div>
          </div>
        <% }) %>
      </ul>
    </main>
  </body>
</html>